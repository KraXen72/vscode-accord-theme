{
	"name": "Accord",
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		// stuff from United Ubuntu / DarkV2
		"badge.foreground": "#ffffff",
		"button.background": "#48b258",
		"button.foreground": "#ffffff",
		"editor.inactiveSelectionBackground": "#3a3d41",
		"editor.lineHighlightBackground": "#11111130",
		"editor.selectionBackground": "#ea542150",
		"editorBracketMatch.background": "#ea542130",
		"editorCursor.foreground": "#ea5421",
		"editorGroupHeader.noTabsBackground": "#111111",
		"editorIndentGuide.background": "#404040",
		"focusBorder": "#ff000000",
		"inputValidation.errorBackground": "#ea2121",
		"inputValidation.infoBackground": "#48a4b2",
		"inputValidation.warningBackground": "#ea7221",
		"list.dropBackground": "#383b3d",
		"scrollbarSlider.background": "#a0391664",
		"scrollbarSlider.hoverBackground": "#802e1264",
		"statusBar.focusBorder": "#ff000000",
		"tab.inactiveForeground": "#cccccc",
		
		// start custom
		"dropdown.background": "#282828",
		"editorHoverWidget.background": "#252526",
		"editorSuggestWidget.background": "#252526",
		"badge.background": "#fb6836", // better badge orange
		"editor.findMatchBackground": "#5caeef4f", //#9e6a03 -> #bb800966
		"editor.findMatchHighlightBackground": "#b485274f",
		"editor.selectionHighlightBackground": "#a5756233",
		"editorGutter.addedBackground": "#587e31",

		// cherry-picked stuff from dark v2
		// leftmost sidebar - activityBar
		"activityBar.background": "#181818",
		"activityBar.border": "#ffffff15",
		"activityBar.foreground": "#ffffffc5",
		"activityBar.inactiveForeground": "#ffffff80",
		"activityBarBadge.foreground": "#ffffff",
		// buttons - secondary
		"button.secondaryBackground": "#FFFFFF0F",
		"button.secondaryForeground": "#ffffffc5",
		"button.secondaryHoverBackground": "#ffffff15",
		// inputs
		"checkbox.background": "#313131",
		"checkbox.border": "#ffffff15",
		"input.background": "#ffffff0f",
		"input.border": "#ffffff17",
		"input.foreground": "#ffffffc5",
		"input.placeholderForeground": "#ffffff79",
		"inputOption.activeBackground": "#2489db82",
		"inputOption.activeBorder": "#2488db",
		// settings
		"settings.dropdownBackground": "#313131",
		"settings.dropdownBorder": "#ffffff17",
		// "settings.headerForeground": "#ffffff",
		// more
		"debugToolBar.background": "#181818",
		"descriptionForeground": "#8b949e",
		"peekViewEditor.background": "#1f1f1f",
		"peekViewEditor.matchHighlightBackground": "#bb800966",
		"peekViewResult.background": "#1f1f1f",
		"peekViewResult.matchHighlightBackground": "#bb800966",

		// dark panels (bottom)
		// "panel.background": "#181818",
		// "panel.border": "#ffffff15",
		// "panelInput.border": "#ffffff15",
		// "panelTitle.activeForeground": "#ffffffc5",
		// "panelTitle.inactiveForeground": "#8b949e",

		// dark quick picker
		"pickerGroup.border": "#ffffff15",
		"quickInput.background": "#1f1f1f",
		"quickInput.foreground": "#ffffffc5",

		// dark sidebars - oh boi
		"sideBar.background": "#181818",
		"sideBarSectionHeader.background": "#181818",
		"sideBar.foreground": "#ffffffc5",
		"sideBarSectionHeader.foreground": "#ffffffc5",
		"sideBarTitle.foreground": "#ffffffc5",
		
		"sideBar.border": "#ffffff15",
		// "sideBarSectionHeader.border": "#ffffff15", // section header sidebar bottom border
		
		// dark statusbar
		"statusBar.background": "#181818",
		"statusBar.border": "#ffffff15",
		"statusBar.debuggingBackground": "#181818",
		// "statusBar.debuggingBackground": "#0078d4",
		// "statusBar.debuggingForeground": "#ffffff",
		"statusBar.foreground": "#ffffffda", // modified
		"statusBar.noFolderBackground": "#181818", // modified
		"statusBarItem.focusBorder": "#0078d4",
		"statusBarItem.prominentBackground": "#6e768166",
		"statusBarItem.remoteBackground": "#6e768100", //#0078d4 -> #6e768166 or #6e768100
		"statusBarItem.remoteForeground": "#ffffffda", // #ffffff -> #ffffffda

		// -- dark + encased tabs --
		"tab.activeBackground": "#1f1f1f",
		// "tab.activeBorder": "#1f1f1f",

		"tab.activeForeground": "#ffffffc5",
		"tab.border": "#ffffff15",
		"tab.hoverBackground": "#1f1f1f",
		"tab.inactiveBackground": "#181818",
		// "tab.inactiveForeground": "#ffffff80",
		"tab.unfocusedActiveBorder": "#1f1f1f",
		"tab.unfocusedHoverBackground": "#6e76811a",
		// tabs + titlebar
		"titleBar.activeBackground": "#181818",
		"titleBar.activeForeground": "#ffffffc5",
		"titleBar.border": "#ffffff15",
		"titleBar.inactiveBackground": "#1f1f1f",
		"titleBar.inactiveForeground": "#8b949e", 
		// titlebar related stuff too
		"editorGroup.border": "#ffffff17",
		"editorGroupHeader.tabsBackground": "#181818",
		"editorGroupHeader.tabsBorder": "#ffffff15",
		"scrollbar.shadow": "#484f5833",

		// dark dropdowns
		// "menu.background": "#1f1f1f",

		// dark notifications
		"notificationCenterHeader.background": "#1f1f1f",
		"notificationCenterHeader.foreground": "#ffffffc5",
		"notifications.background": "#1f1f1f",
		"notifications.border": "#ffffff15",
		"notifications.foreground": "#ffffffc5", 

		// dark terminal stuff?
		"terminal.foreground": "#ffffffc5",
		// "terminal.foreground": "#ff0000",

		// no effect?asdfasdfdas
		"editorWidget.background": "#1f1f1f",
		// "errorForeground": "#f85149",
		// "focusBorder": "#0078d4",
		//no effect, don't hurt
		"foreground": "#ffffffc5",
		"icon.foreground": "#ffffffc5",
		"editor.background": "#1f1f1f",
		"editor.foreground": "#ffffffd3",

		"keybindingLabel.foreground": "#ffffffc5",

		// blue line numbers
		// "editorLineNumber.activeForeground": "#ffffffc5",
		// "editorLineNumber.foreground": "#6e7681",
		// "editorOverviewRuler.border": "#010409",

		// custom bracket colors from nord
		"editorBracketHighlight.foreground1": "#abb2c0",
		"editorBracketHighlight.foreground2": "#5caeef",
		"editorBracketHighlight.foreground3": "#4fb1bc",
		"editorBracketHighlight.foreground4": "#c172d9",
		"editorBracketHighlight.foreground5": "#97c26c",
		"editorBracketHighlight.foreground6": "#dfb976",
		"editorBracketHighlight.unexpectedBracket.foreground": "#db6165",

		// green apple accent color #7CB342. complementary color is #427cb3, analogous is #42b353
		// i always thought i hate green, well, yet, here were we are.
		"activityBarBadge.background": "#7CB342",
		"list.activeSelectionForeground": "#7CB342",
		"list.inactiveSelectionForeground": "#7CB342",
		"list.highlightForeground": "#7CB342",
		"editorSuggestWidget.highlightForeground": "#7CB342",
		"textLink.foreground": "#7CB342",
		"progressBar.background": "#7CB342",
		"pickerGroup.foreground": "#7CB342",
		"tab.activeBorder": "#7CB342",
		"tab.activeBorderTop": "#7CB34200", // optional
		"tab.unfocusedActiveBorderTop": "#7CB342",
		"notificationLink.foreground": "#7CB342",
		"editorWidget.resizeBorder": "#7CB342",
		"editorWidget.border": "#7CB342",
		"settings.modifiedItemIndicator": "#7CB342",
		"settings.headerForeground": "#7CB342",
		"panelTitle.activeBorder": "#7CB342",
		"breadcrumb.activeSelectionForeground": "#7CB342",
		"menu.selectionForeground": "#7CB342",
		"menubar.selectionForeground": "#7CB342",
		"scrollbarSlider.activeBackground": "#7CB34250",
		"terminal.tab.activeBorder": "#7CB342"
	},
	"tokenColors": [
		{
			"name": "plain",
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#ffffffd3"
			}
		},
		{
			"name": "classes",
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "interfaces",
			"scope": [
				"entity.name.type.interface",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "structs",
			"scope": [
				"entity.name.type.struct"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "enums",
			"scope": [
				"entity.name.type.enum"
			],
			"settings": {
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "keys",
			"scope": [
				"meta.object-literal.key",
				"support.type.property-name",
				"keyword.other.definition.ini"
			],
			"settings": {
				"foreground": "#96cbfe"
			}
		},
		{
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#c6c5fe"
			}
		},
		{
			"name": "css property",
			"scope": [
				"suppport.type.property-name.css"
			],
			"settings": {
				"foreground": "#c6c5fe",
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#e9c062"
			}
		},
		{
			"name": "methods",
			"scope": [
				"entity.name.function.method",
				"meta.function.method"
			],
			"settings": {
				"foreground": "#96cbfe"
			}
		},
		{
			"name": "functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#96cbfe",
			}
		},
		{
			"name": "variables",
			"scope": [
				"variable",
				"meta.variable",
				"variable.other.object.property"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"scope": [
				"variable.argument.css"
			],
			"settings": {
				"foreground": "#ff6c60",
			}
		},
		{
			"name": "variablesOther",
			"scope": [
				"variable.other.object",
				"variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"name": "globalVariables",
			"scope": [
				"variable.other.global",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#ff6c60",
			}
		},
		{
			"name": "italic this/self",
			"scope": "variable.language.this",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "localVariables",
			"scope": [
				"variable.other.local"
			],
			"settings": {
				"foreground": "#282726"
			}
		},
		{
			"name": "parameters",
			"scope": [
				"variable.parameter",
				"meta.parameter"
			],
			"settings": {
				"foreground": "#e9c062"
			}
		},
		{
			"name": "properties",
			"scope": [
				"variable.other.property",
				"meta.property"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"name": "strings",
			"scope": [
				"string",
				"string.other.link",
				"markup.inline.raw.string.markdown",
				"markup.inline.raw.code.mdx"
			],
			"settings": {
				"foreground": "#a8ff60"
			}
		},
		{
			"name": "stringEscapeSequences",
			"scope": [
				"constant.character.escape",
				"constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"name": "keywords",
			"scope": [
				"keyword",
				"keyword.other.directive.using"
			],
			"settings": {
				"foreground": "#ff73fd"
			}
		},
		{
			"name": "keywordsControl",
			"scope": [
				"keyword.control",
				"keyword.control.as",
				"keyword.control.import",
				"keyword.control.from",
				"keyword.import",
			],
			"settings": {
				"foreground": "#b5b3aa"
			}
		},
		{
			"name": "storageModifiers",
			"scope": [
				"storage.modifier",
				"keyword.modifier",
				"storage.type"
			],
			"settings": {
				"foreground": "#ff73fd"
			}
		},
		{
			"name": "comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#8bbd92",
				"fontStyle": "italic"
			}
		},
		{
			"name": "docComments",
			"scope": [
				"comment.documentation",
				"comment.line.documentation"
			],
			"settings": {
				"foreground": "#8bbd92",
				"fontStyle": "italic"
			}
		},
		{
			"name": "numbers",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#e9c062"
			}
		},
		{
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#ce9178"
			}
		},
		{
			"name": "booleans",
			"scope": [
				"constant.language.boolean",
				"constant.language.json"
			],
			"settings": {
				"foreground": "#e9c062"
			}
		},
		{
			"name": "operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"name": "special operators",
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression"
			],
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"name": "special punctuation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"name": "macros",
			"scope": [
				"entity.name.function.preprocessor",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#96cbfe"
			}
		},
		{
			"name": "urls",
			"scope": [
				"markup.underline.link",
				"string.other.link.destination.mdx"
			],
			"settings": {
				"foreground": "#96cbfe"
			}
		},
		{
			"name": "tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#d7ba7d"
			}
		},
		{
			"name": "jsxTags",
			"scope": [
				"support.class.component"
			],
			"settings": {
				"foreground": "#ffffb6"
			}
		},
		{
			"name": "attributes",
			"scope": [
				"entity.other.attribute-name",
				"meta.attribute"
			],
			"settings": {
				"foreground": "#96cbfe"
			}
		},
		{
			"name": "types",
			"scope": [
				"support.type",
				"variable.legacy.builtin.python"
			],
			"settings": {
				"foreground": "#3aa99f"
			}
		},
		{
			"name": "constants",
			"scope": [
				"variable.other.constant",
				"variable.readonly"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"name": "namespaces",
			"scope": [
				"entity.name.namespace",
				"storage.modifier.namespace",
				"markup.bold.markdown",
				"entity.name.scope-resolution"
			],
			"settings": {
				"foreground": "#ffffb6"
			}
		},
		{
			"name": "typeParameters - UNSURE",
			"scope": [
				"variable.type.parameter",
				"variable.parameter.type"
			],
			"settings": {
				"foreground": "#e9c062"
			}
		},
		{
			"name": "exceptions",
			"scope": [
				"keyword.control.exception",
				"keyword.control.trycatch"
			],
			"settings": {
				"foreground": "#ff73fd"
			}
		},
		{
			"name": "decorators",
			"scope": [
				"meta.decorator",
				"punctuation.decorator",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#e9c062"
			}
		},
		{
			"name": "calls",
			"scope": [
				"variable.function"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"name": "punctuation",
			"scope": [
				"punctuation",
				"punctuation.terminator",
				"punctuation.definition.tag",
				"punctuation.separator",
				"punctuation.definition.string",
				"punctuation.section.block"
			],
			"settings": {
				"foreground": "#ffffffc5"
			}
		},
		{
			"name": "orange IR09",
			"scope": [
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"constant.language.go",
				"support.class.dart",
				"keyword.other.documentation",
				"storage.modifier.import.java",
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.quote.begin.markdown",
				"meta.separator.markdown",
				"entity.name.section.markdown",
				"entity.name.section.mdx",
				"string.other.number.mdx",
				"variable.ordered.list.mdx",
				"variable.unordered.list.mdx"
			],
			"settings": {
				"foreground": "#e9c062"
			}
		},
		{
			"name": "green IR0B",
			"scope": [],
			"settings": {
				"foreground": "#a8ff60"
			}
		},
		{
			"name": "blue IR0D",
			"scope": [],
			"settings": {
				"foreground": "#96cbfe"
			}
		},
		{
			"name": "magenta",
			"scope": [
				"punctuation.definition.heading.markdown",
				"punctuation.definition.heading.mdx"
			],
			"settings": {
				"foreground": "#ff73fd"
			}
		}
	]
