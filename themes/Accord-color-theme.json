	"tokenColors": [
		{
			"scope": [ "source"],
			"settings": {
				"foreground": "#FFFFFFD3"
			}
		},
		{
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#FFFFFFC5"
			}
		},
		{
			"name": "keywords",
			"scope": [
				"keyword",
				"keyword.other.directive.using",
				"keyword.control.exception",
				"keyword.control.flow",
				"keyword.control.return",
				"keyword.control.trycatch",
				"keyword.modifier",
				"storage.modifier",
				"storage.type",
			],
			"settings": {
				"foreground": "#FF73FD"
			}
		},
		{
			"name": "special keywords",
			"scope": [
				"keyword.operator.new",
				"keyword.control.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python",
			],
			"settings": {
				"foreground": "#569CD6"
			}
		},
		// {
		// 	"name": "differentiate = and ==",
		// 	"scope": [ "keyword.operator.assignment"  ],
		// 	"settings": {
		// 		"foreground": "#569CD6",
		// 	}
		// },
		{
			"name": "variables",
			"scope": [
				"variable",
				"meta.variable",
				"variable.other.object.property",
				"variable.other.object",
				"variable.other.readwrite.alias",
				"variable.other.property",
				"variable.other.constant",
				"meta.property",
				"variable.readonly"
			],
			"settings": {
				"foreground": "#FFFFFFC5"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"scope": [
				"variable.argument.css",
				"variable.other.global",
				// unsure
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js",
			],
			"settings": {
				"foreground": "#FF6C60"
			}
		},
		{
			"scope": [
				"entity.name.type.enum",
				"entity.name.type.struct",
				"entity.name.type.interface"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"name": "primitive types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#C6C5FE",
			}
		},
		{
			"name": "complex types",
			"scope": [
				"meta.return-type",
				"entity.name.type",
				"entity.name.class",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"name": "classes",
			"scope": [
				"support.class",
				"support.class.dart",
				"entity.name.type.class",
				"entity.name.type.module",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#FFFFB6"
			}
		},
		{
			"name": "imports",
			"scope": [
				"keyword.control",
				"keyword.control.as",
				"keyword.control.import",
				"keyword.control.from",
				"keyword.import",
				"keyword.other.import",
				"storage.modifier.import.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#B5B3AA"
			}
		},
		{
			"name": "functions, methods",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"meta.function.method",
				"meta.function-call.generic",
				"meta.method.declaration meta.definition.method",
			],
			"settings": {
				"foreground": "#96CBFE"
			}
		},
		{
			"name": "keys",
			"scope": [
				"meta.object-literal.key",
				"support.type.property-name",
				"keyword.other.definition.ini",
				"source.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#96CBFE"
			}
		},
		{
			"name": "attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#9CDCFE"
			}
		},
		{
			"name": "comments",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"comment.documentation",
				"comment.line.documentation",
				// docstrings styled as comments:
				// "source.python string.quoted.docstring",
				// "source.python string.quoted.docstring punctuation.definition.string"
			],
			"settings": {
				"foreground": "#8BBD92",
				"fontStyle": "italic"
			}
		},
		{
			"name": "strings",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
				"string.other.link"
			],
			"settings": {
				"foreground": "#A8FF60"
			}
		},

		// -----
